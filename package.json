{
  "name": "react-fullstack",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node lib/bin/server",
    "start:api": "node lib/apiServer", 
    "build": "clear lib && cross-env BABEL_ENV=node babel src -d lib --copy-files",
    "build:watch": "npm run build -- --watch",
    "serve": "concurrently --kill-others-on-fail --names \" BUILD , BUNDLE , SERVE, SERVE-API \" -c \"bgMagenta.bold,bgCyan.bold,bgRed.bold,bgGreen.bold\" --prefix \"{name}\" \"npm run build:watch\" \"npm run bundle\" \"npm run start\" \"npm run start:api\" ",
    "bundle": "clear dist && cross-env BABEL_ENV=es6 webpack --progress --colors"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "http-proxy": "^1.16.2",
    "i": "^0.3.5",
    "jade": "~1.11.0",
    "mongoose": "^4.11.10",
    "morgan": "~1.8.1",
    "nodemon": "^1.12.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "redux-logger": "^3.0.6",
    "webpack": "^3.5.5"
  }
}
